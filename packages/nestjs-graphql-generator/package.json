{
  "name": "@cenk1cenk2/nestjs-graphql-generator",
  "version": "1.0.0",
  "description": "A library using EventEmitter2 to have auto typing properties.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/cenk1cenk2/nestjs-tools",
  "author": {
    "name": "Cenk Kilic",
    "email": "cenk@kilic.dev",
    "url": "https://srcs.kilic.dev"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build": "tsup-node",
    "dev:start": "tsup-node --watch --silent",
    "lint": "prettier --write src/ --loglevel warn && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:api": "typedoc --options .typedoc.json --hideInPageTOC --hideBreadcrumbs"
  },
  "peerDependencies": {
    "config": ">= 3.3.3",
    "graphql": ">= 15.4.0"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/plugin-helpers": "^2.4.2",
    "delay": "^5.0.0"
  },
  "devDependencies": {
    "@types/config": "^0.0.41",
    "config": "^3.3.7",
    "graphql": "^16.5.0"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  }
}
