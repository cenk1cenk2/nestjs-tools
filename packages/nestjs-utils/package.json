{
  "name": "@cenk1cenk2/nestjs-utils",
  "version": "1.0.0",
  "description": "A general set of utilities for nestjs.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/cenk1cenk2/nestjs-tools",
  "author": {
    "name": "Cenk Kilic",
    "email": "cenk@kilic.dev",
    "url": "https://srcs.kilic.dev"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --write src/ --loglevel warn && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:api": "typedoc --options .typedoc.json --hideInPageTOC --hideBreadcrumbs"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 7.6.1 < 10",
    "@webundsoehne/nestjs-util": ">= 2.3.2 < 8",
    "winston": ">= 3.3.3 < 4"
  },
  "devDependencies": {
    "@nestjs/common": "^9.3.10",
    "@webundsoehne/nestjs-util": "^7.1.3",
    "winston": "^3.8.2"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "dependencies": {
    "colorette": "^2.0.19"
  }
}
