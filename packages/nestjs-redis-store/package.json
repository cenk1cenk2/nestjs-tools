{
  "name": "@cenk1cenk2/nestjs-redis-store",
  "version": "1.0.0",
  "description": "A nestjs extension to use Redis as a database.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/cenk1cenk2/nestjs-tools",
  "author": {
    "name": "Cenk Kilic",
    "email": "cenk@kilic.dev",
    "url": "https://srcs.kilic.dev"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --write src/ --loglevel warn && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:api": "typedoc --options .typedoc.json --hideInPageTOC --hideBreadcrumbs"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 7.6.1",
    "ioredis": ">= 4.27.9",
    "redis-om": ">= 0.3.0"
  },
  "peerDependenciesMeta": {
    "redis-om": {
      "optional": true
    },
    "io-redis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/common": "^9.3.12",
    "@types/ioredis": "^5.0.0",
    "ioredis": "^5.3.1",
    "redis-om": "^0.3.6"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  }
}
