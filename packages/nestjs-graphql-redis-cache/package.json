{
  "name": "@cenk1cenk2/nestjs-graphql-redis-cache",
  "version": "1.0.0",
  "description": "Redis cache manager for nestjs graphql.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/cenk1cenk2/nestjs-tools",
  "author": {
    "name": "Cenk Kilic",
    "email": "cenk@kilic.dev",
    "url": "https://srcs.kilic.dev"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build": "tsup-node",
    "dev:start": "tsup-node --watch --silent",
    "lint": "prettier --write src/ --loglevel warn && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:api": "typedoc --options .typedoc.json --hideInPageTOC --hideBreadcrumbs"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 7.6.1 < 10",
    "@nestjs/core": ">= 8",
    "@nestjs/graphql": ">= 8",
    "@webundsoehne/nestjs-util": ">= 2.3.2",
    "cache-manager": ">= 3.4.4",
    "cache-manager-ioredis": ">= 2.0.0",
    "ioredis": ">= 4.27.9",
    "rxjs": ">= 7.3.1"
  },
  "devDependencies": {
    "@nestjs/common": "^9.1.4",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/ioredis": "^4.28.10",
    "@webundsoehne/nestjs-util": "^6.0.1",
    "cache-manager": "^5.0.1",
    "cache-manager-ioredis": "^2.1.0",
    "ioredis": "^5.2.3"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  }
}
